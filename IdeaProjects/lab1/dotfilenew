digraph graphname{ 
and->new[ label = 1 ]; 
new->civilizations[ label = 1 ]; 
new->life[ label = 1 ]; 
new->worlds[ label = 1 ]; 
worlds->to[ label = 1 ]; 
to->explore[ label = 1 ]; 
to->find[ label = 1 ]; 
to->seek[ label = 1 ]; 
seek->out[ label = 1 ]; 
out->good[ label = 1 ]; 
out->new[ label = 1 ]; 
find->out[ label = 1 ]; 
explore->strange[ label = 1 ]; 
strange->new[ label = 1 ]; 
life->and[ label = 1 ]; 
civilizations->to[ label = 1 ]; 
good; 

to -> find[ label = 1, color = blue ] 

find -> out[ label = 1, color = blue ] 

to -> seek[ label = 1, color = green ] 

seek -> out[ label = 1, color = green ] 
}
